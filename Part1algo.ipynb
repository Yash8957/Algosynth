        "\n",
        "# Part 1: Parity Checker for a Single Number\n",
        "\n",
        "# Ask the user to enter a number\n",
        "number = int(input(\"Enter a number: \"))\n",
        "\n",
        "# Check if the number is even or odd using modulo operator\n",
        "if number % 2 == 0:\n",
        "    print(\"Even\")\n",
        "else:\n",
        "    print(\"Odd\")\n"
      ]
    }
  ]
